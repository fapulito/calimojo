# Facebook Auth Configuration
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_CALLBACK_URL=/auth/facebook/callback

# Session Configuration
SESSION_SECRET=your_session_secret_here
SESSION_MAX_AGE=86400000 # 24 hours in milliseconds

# Server Configuration
PORT=3000
NODE_ENV=development

# NeonDB PostgreSQL Configuration
NEONDB_CONNECTION_STRING=postgres://user:password@host:port/database
DATABASE_URL=postgres://user:password@host:port/database

# Database Configuration
DB_HOST=your-neondb-host
DB_USER=your-neondb-user
DB_PASSWORD=your-neondb-password
DB_NAME=your-neondb-database-name
DB_PORT=5432
DB_SSL=true

# Session Store Configuration
SESSION_STORE=postgres
SESSION_TABLE=sessions